{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"baseUrl": ".",
		"allowSyntheticDefaultImports": true,
		"paths": {
			"@/*": ["src/*"]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react"
	}
}
